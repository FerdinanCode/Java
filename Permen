package kelas5b;

/**
 *
 * @author Ferdinan
 */
public class Permen {
    public void tambahPermen() {
        int jumlahPermen = 0;
        jumlahPermen = jumlahPermen + 1;
        System.out.println("jumlah permen setelah ditambah : " + jumlahPermen);
    }
    public static void main(String[] args) {
        Permen permenSusu = new Permen();
        permenSusu.tambahPermen();
    }
    
}
