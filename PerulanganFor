package kelas5b;

/**
 *
 * @author LAB04-03
 */
public class PerulanganFor {
    public static void main(String[] args) {
        System.out.println("For Ascending");
        for (int i = 1; i <= 5; i++) {
            System.out.println(i);
        }
        System.out.println("For Ascending");
        for (int i = 5; i <= 1; i--) {
            System.out.println(i);
        }
    }
}
    
