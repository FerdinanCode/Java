package fedinan11;

public class Perulangan6 {
    public int nilai1;
    
    public int hitungPerulangan() {
        System.out.print(nilai1);
        
        for (int i = 3; i <=7 ; i++) {
            int hasil = nilai1 - i;
            System.out.print(""+hasil);
            nilai1 = hasil;
        }
        System.out.println();
        return nilai1;
    }
}
