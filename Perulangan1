public class Perulangan1 {
    public int nilai ;
    public int hitungPerulangan(){
        System.out.print( nilai + " ");
        for (int i = 1; i <=10; i++) {
            int hasil = nilai + (i * i);
            System.out.print( hasil + " ");
            nilai = hasil;
        }
        System.out.println();
        return nilai;
    }
}    
