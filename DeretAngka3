public class DeretAngka3 {
    public static void main(String[] args) {
        System.out.println("Membuat Deret Angka");
        int C = 0;
        int C1 = 3;
        int count = 22;
        for (int i = 1; i <= count; i++) {
            System.out.println(C + " ");
            int sum = C + C1;
            C = C1;
            C1 = sum;
        }
    }
}
