package kelas5b;

/**
 *
 * @author LAB04-03
 */
public class PerulanganWhile {
    public static void main(String[] args) {
        int i = 2;
        while (i <= 20) {
        System.out.println(i);
                
        i = i + 2;
        }
    }
}
    
