public class Omnivora extends Binatang {
    private String makananutama;
    private String habitat;
    
    public String getMakananUtama(){
        return makananutama;
    }
    public void setMakananUtama (String makananutama){
        this.makananutama = makananutama;
    }
    public String getHabitat(){
        return habitat;
    }
    public void setHabitat(String habitat){
        this.habitat = habitat;
    }
    public void karnivoraApa(){
        System.out.println(" ID Hewan  :" + id);
        System.out.println(nama + " berumur " + umur + " tahun dengan berat " + berat + " kg " + " memiliki tinggi " + tinggi + " cm " );
        System.out.println(" Makanan Utama : " + makananutama);
        System.out.println(" Habitat : " + habitat);
    }
    }
