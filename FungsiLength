package kelas5b;

/**
 *
 * @author LAB04-03
 */
public class FungsiLength {
    public static void main(String[] args) {
        String nama = "Ferdinan";
        
        int jumlahKarakter = nama.length();
        System.out.println("'" + nama + "'mempunyai jumlah karakter" + "sebanyak" + jumlahKarakter);
    }
    
}
